{
  "name": "angular2-typescript-webpack",
  "version": "1.0.0",
  "description": "Angular 2 with TypeScript and Webpack",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server --config webpack.dev.config.js --inline --progress --port 3000",
    "build": "rimraf dist && webpack --config webpack.prod.config.js --progress --profile --bail",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Angular 2",
    "Typescript",
    "Webpack"
  ],
  "author": "roel barreto <greatcodeideas@gmail.com>",
